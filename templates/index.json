n}\n@media (max-width:968px){\n  .results-box{\n    grid-template-columns:1fr;\n  }\n  .section-title{\n    font-size:32px;\n  }\n  .stats-grid{\n    gap:50px;\n  }\n}\n@media (max-width:640px){\n  .transformational-section{\n    padding:60px 15px;\n  }\n  .section-title{\n    font-size:28px;\n  }\n  .results-content{\n    padding:30px 20px;\n  }\n  .tabs-row{\n    flex-direction:column;\n  }\n  .tab-btn{\n    margin-right:0;\n    margin-bottom:-1px;\n  }\n  .stat-number{\n    font-size:42px;\n  }\n  .stats-grid{\n    gap:40px;\n  }\n}\n</style>\n\n<div class=\"transformational-section\" id=\"transformational-section\">\n  <div class=\"section-header\">\n    <div class=\"section-tag\">[ SCIENCE & R√âSULTATS ]</div>\n    <h2 class=\"section-title\">PUISSANCE NATURELLE</h2>\n  </div>\n\n  <div class=\"results-container\">\n    <div class=\"results-box\">\n      <div class=\"results-image\">\n        <img id=\"img-gut\" src=\"https://cdn.shopify.com/s/files/1/0713/6952/3289/files/O_ORIGINE-20.svg?v=1770676555\" alt=\"Sant√© Intestinale\">\n        <img id=\"img-performance\" src=\"https://cdn.shopify.com/s/files/1/0713/6952/3289/files/imgi_43_image_80_1.jpg?v=1770676610\" alt=\"Performance\">\n        <img id=\"img-skin\" class=\"active\" src=\"https://cdn.shopify.com/s/files/1/0713/6952/3289/files/Capture_d_ecran_le_2026-02-09_a_17.52.41.png?v=1770677567\" alt=\"Peau & Cheveux\">\n      </div>\n      <div class=\"results-content\">\n        <div class=\"tabs-row\">\n          <button class=\"tab-btn\" data-tab=\"gut\">SANT√â INTESTINALE</button>\n          <button class=\"tab-btn\" data-tab=\"performance\">PERFORMANCE</button>\n          <button class=\"tab-btn active\" data-tab=\"skin\">PEAU & CHEVEUX</button>\n        </div>\n        \n        <div id=\"gut\" class=\"tab-content\">\n          <h3 class=\"content-title\">‚ñº Sant√© Intestinale</h3>\n          <p class=\"content-text\">OORIGINE Colostrum‚Ñ¢ favorise la sant√© digestive en fortifiant naturellement les quatre couches de votre barri√®re muqueuse intestinale, y compris la paroi intestinale, les cellules immunitaires et anticorps, la couche de mucus et l'√©cosyst√®me intestinal.</p>\n        </div>\n        \n        <div id=\"performance\" class=\"tab-content\">\n          <h3 class=\"content-title\">+ Performance</h3>\n          <p class=\"content-text\">OORIGINE Colostrum‚Ñ¢ est une source naturelle de plus de 400 nutriments bioactifs, soutenant la performance et la r√©cup√©ration fitness. Les facteurs de croissance, comme IGF-1, soutiennent l'√©quilibre sain du glucose et alimentent le m√©tabolisme cellulaire.</p>\n        </div>\n        \n        <div id=\"skin\" class=\"tab-content active\">\n          <h3 class=\"content-title\">‚ú¶ Peau & Cheveux</h3>\n          <p class=\"content-text\">OORIGINE Colostrum‚Ñ¢ offre une matrice de plus de 400 nutriments bioactifs, incluant exosomes, antioxydants, pr√©biotiques alimentaires entiers, facteurs de croissance, vitamines et peptides r√©g√©n√©ratifs.</p>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"stats-grid\">\n      <div class=\"stat-box\">\n        <div class=\"stat-number\" data-target=\"79\">0%</div>\n        <div class=\"stat-label\">Ont signal√© des cheveux plus √©pais‚Ä†</div>\n      </div>\n      <div class=\"stat-box\">\n        <div class=\"stat-number\" data-target=\"80\">0%</div>\n        <div class=\"stat-label\">Ont signal√© une diminution des rides et une am√©lioration de leurs pr√©occupations cutan√©es‚Ä†</div>\n      </div>\n    </div>\n  </div>\n</div>\n\n<script>\nvar hasAnimated = false;\n\nvar statsData = {\n  gut: [\n    {number: 86, label: 'Ont ressenti moins de ballonnements‚Ä†'},\n    {number: 79, label: 'Ont signal√© moins de gaz et un meilleur bien-√™tre digestif global‚Ä†'},\n    {number: 75, label: 'Ont signal√© une am√©lioration significative de la r√©gularit√©‚Ä†'},\n    {number: 71, label: 'Ont signal√© moins d\\'inconfort intestinal apr√®s les repas‚Ä†'}\n  ],\n  performance: [\n    {number: 83, label: 'Ont signal√© une am√©lioration de la r√©cup√©ration fitness*'},\n    {number: 83, label: 'Ont signal√© se sentir moins endoloris apr√®s l\\'entra√Ænement*'},\n    {number: 77, label: 'Ont signal√© se sentir moins √©puis√©s apr√®s l\\'effort*'}\n  ],\n  skin: [\n    {number: 79, label: 'Ont signal√© des cheveux plus √©pais‚Ä†'},\n    {number: 80, label: 'Ont signal√© une diminution des rides et une am√©lioration de leurs pr√©occupations cutan√©es‚Ä†'}\n  ]\n};\n\nfunction animateNumber(element, target) {\n  var current = 0;\n  var increment = 1;\n  var delay = 2000 / target;\n  \n  var timer = setInterval(function() {\n    current += increment;\n    element.textContent = current + '%';\n    if (current >= target) {\n      element.textContent = target + '%';\n      clearInterval(timer);\n    }\n  }, delay);\n}\n\nfunction animateAllStats() {\n  var statNumbers = document.querySelectorAll('.transformational-section .stat-number');\n  statNumbers.forEach(function(el) {\n    var target = parseInt(el.getAttribute('data-target'));\n    if (target > 0) {\n      el.textContent = '0%';\n      animateNumber(el, target);\n    }\n  });\n}\n\nfunction switchResultTab(tabName) {\n  document.querySelectorAll('.tab-btn').forEach(function(btn) {\n    btn.classList.remove('active');\n  });\n  document.querySelectorAll('.tab-content').forEach(function(content) {\n    content.classList.remove('active');\n  });\n  document.querySelectorAll('.results-image img').forEach(function(img) {\n    img.classList.remove('active');\n  });\n  \n  document.querySelector('[data-tab=\"' + tabName + '\"]').classList.add('active');\n  document.getElementById(tabName).classList.add('active');\n  document.getElementById('img-' + tabName).classList.add('active');\n  \n  var statsGrid = document.querySelector('.stats-grid');\n  statsGrid.innerHTML = statsData[tabName].map(function(stat) {\n    return '<div class=\"stat-box\"><div class=\"stat-number\" data-target=\"' +