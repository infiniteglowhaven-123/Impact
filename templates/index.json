ight: 0;\n    opacity: 0.4;\n  }\n  \n  .quote-mark.opening {\n    margin-bottom: 15px;\n    display: block;\n  }\n  \n  .testimonial-text {\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;\n    font-size: 15px;\n    line-height: 1.7;\n    color: #4a4a4a;\n    margin: 0 0 15px 0;\n  }\n  \n  .quote-mark.closing {\n    text-align: right;\n    display: block;\n    margin-top: 10px;\n  }\n  \n  .testimonial-author {\n    text-align: center;\n    margin-top: 25px;\n    padding-top: 20px;\n    border-top: 1px solid #e8e8e8;\n  }\n  \n  .author-divider {\n    width: 40px;\n    height: 2px;\n    background: #d4af37;\n    margin: 0 auto 15px;\n  }\n  \n  .author-info {\n    font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;\n    font-size: 14px;\n    color: #666;\n  }\n  \n  .nav-button {\n    width: 48px;\n    height: 48px;\n    border-radius: 50%;\n    background: white;\n    border: 2px solid #e8e8e8;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    cursor: pointer;\n    transition: all 0.3s ease;\n    flex-shrink: 0;\n    z-index: 10;\n  }\n  \n  .nav-button:hover {\n    border-color: #d4af37;\n    background: #d4af37;\n    color: white;\n  }\n  \n  .nav-button:disabled {\n    opacity: 0.3;\n    cursor: not-allowed;\n  }\n  \n  .nav-button:disabled:hover {\n    border-color: #e8e8e8;\n    background: white;\n    color: #000;\n  }\n  \n  .nav-button svg {\n    width: 20px;\n    height: 20px;\n  }\n  \n  /* MOBILE STYLES */\n  @media (max-width: 968px) {\n    .testimonials-section {\n      padding: 50px 15px;\n    }\n    \n    .testimonials-title {\n      font-size: 36px;\n    }\n    \n    .testimonials-subtitle {\n      font-size: 15px;\n    }\n    \n    .testimonials-wrapper {\n      gap: 15px;\n    }\n    \n    .testimonials-track {\n      gap: 20px;\n    }\n    \n    .testimonial-card {\n      width: 100%;\n      padding: 30px 25px;\n    }\n    \n    .testimonial-title {\n      font-size: 20px;\n    }\n    \n    .testimonial-text {\n      font-size: 14px;\n    }\n    \n    .nav-button {\n      width: 40px;\n      height: 40px;\n    }\n    \n    .nav-button svg {\n      width: 16px;\n      height: 16px;\n    }\n  }\n  \n  @media (max-width: 640px) {\n    .testimonials-title {\n      font-size: 28px;\n    }\n    \n    .testimonials-header {\n      margin-bottom: 40px;\n    }\n  }\n  \n  /* Desktop grand √©cran */\n  @media (min-width: 1400px) {\n    .testimonials-title {\n      font-size: 44px;\n    }\n    \n    .testimonials-subtitle {\n      font-size: 13px;\n    }\n  }\n</style>\n\n<div class=\"testimonials-section\">\n  <div class=\"testimonials-container\">\n    <div class=\"testimonials-header\">\n      <h2 class=\"testimonials-title\">Wear Your Confidence</h2>\n      <p class=\"testimonials-subtitle\">Real stories from women who chose to shine</p>\n    </div>\n    \n    <div class=\"testimonials-wrapper\">\n      <button class=\"nav-button\" id=\"prevBtn\" aria-label=\"Previous testimonials\">\n        <svg fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n          <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M15 19l-7-7 7-7\"/>\n        </svg>\n      </button>\n      \n      <div class=\"testimonials-slider\">\n        <div class=\"testimonials-track\" id=\"track\">\n          <!-- Testimonial 1 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Best jewelry purchase ever</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              Bought the ring set for my birthday and couldn't be happier. They're so comfortable I forget I'm wearing them. Great value for the quality you get.\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Camille ‚Ä¢ Chicago, IL</p>\n            </div>\n          </div>\n          \n          <!-- Testimonial 2 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Stylish and durable</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              Love how these pieces look so expensive but are actually affordable. The earrings are lightweight and comfortable for all-day wear. Already planning my next order!\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Jessica ‚Ä¢ Denver, CO</p>\n            </div>\n          </div>\n          \n          <!-- Testimonial 3 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Obsessed with layering these</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              Got the chain set and now I can't stop layering them. They look so elegant together and haven't tarnished at all. Perfect for dressing up or keeping it casual.\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Aisha ‚Ä¢ Seattle, WA</p>\n            </div>\n          </div>\n          \n          <!-- Testimonial 4 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Great customer service</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              Had a question about sizing and they responded so quickly. The bracelet fits perfectly and I love how it catches the light. Definitely ordering more pieces.\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Lena ‚Ä¢ Austin, TX</p>\n            </div>\n          </div>\n          \n          <!-- Testimonial 5 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Perfect for everyday wear</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              I wear my necklace everywhere - work, gym, beach trips. It still looks brand new after months of daily wear. The quality really shows and I always get compliments.\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Maya ‚Ä¢ Los Angeles, CA</p>\n            </div>\n          </div>\n          \n          <!-- Testimonial 6 -->\n          <div class=\"testimonial-card\">\n            <span class=\"verified-badge\">VERIFIED</span>\n            <div class=\"stars\">‚òÖ ‚òÖ ‚òÖ ‚òÖ ‚òÖ</div>\n            <h3 class=\"testimonial-title\">Love the quality!</h3>\n            <span class=\"quote-mark opening\">\"</span>\n            <p class=\"testimonial-text\">\n              Finally found jewelry that doesn't turn my skin green or fade. The hoops are my go-to - comfortable enough to sleep in and they go with everything in my wardrobe.\n            </p>\n            <span class=\"quote-mark closing\">\"</span>\n            <div class=\"testimonial-author\">\n              <div class=\"author-divider\"></div>\n              <p class=\"author-info\">Sofia ‚Ä¢ Miami, FL</p>\n            </div>\n          </div>\n        </div>\n      </div>\n      \n      <button class=\"nav-button\" id=\"nextBtn\" aria-label=\"Next testimonials\">\n        <svg fill=\"none\" stroke=\"currentColor\" viewBox=\"0 0 24 24\">\n          <path stroke-linecap=\"round\" stroke-linejoin=\"round\" stroke-width=\"2\" d=\"M9 5l7 7-7 7\"/>\n        </svg>\n      </button>\n    </div>\n  </div>\n</div>\n\n<script>\n  (function() {\n    const track = document.getElementById('track');\n    const prevBtn = document.getElementById('prevBtn');\n    const nextBtn = document.getElementById('nextBtn');\n    let currentIndex = 0;\n    const totalCards = 6;\n    let touchStartX = 0;\n    let touchEndX = 0;\n    \n    function isMobile() {\n      return window.innerWidth <= 968;\n    }\n    \n    function getCardsVisible() {\n      return isMobile() ? 1 : 2;\n    }\n    \n    function getCardWidth() {\n      const card = document.querySelector('.testimonial-card');\n      const gap = isMobile() ? 20 : 40;\n      return card.offsetWidth + gap;\n    }\n    \n    function updateSlide() {\n      const cardWidth = getCardWidth();\n      const offset = currentIndex * cardWidth;\n      track.style.transform = `translateX(-${offset}px)`;\n      \n      const cardsVisible = getCardsVisible();\n      prevBtn.disabled = currentIndex === 0;\n      nextBtn.disabled = currentIndex >= totalCards - cardsVisible;\n    }\n    \n    prevBtn.addEventListener('click', () => {\n      if (currentIndex > 0) {\n        currentIndex--;\n        updateSlide();\n      }\n    });\n    \n    nextBtn.addEventListener('click', () => {\n      const cardsVisible = getCardsVisible();\n      if (currentIndex < totalCards - cardsVisible) {\n        currentIndex++;\n        updateSlide();\n      }\n    });\n    \n    // Support du swipe sur mobile\n    track.addEventListener('touchstart', (e) => {\n      touchStartX = e.changedTouches[0].screenX;\n    });\n    \n    track.addEventListener('touchend', (e) => {\n      touchEndX = e.changedTouches[0].screenX;\n      handleSwipe();\n    });\n    \n    function handleSwipe() {\n      if (touchEndX < touchStartX - 50) {\n        // Swipe left - next\n        const cardsVisible = getCardsVisible();\n        if (currentIndex < totalCards - cardsVisible) {\n          currentIndex++;\n          updateSlide();\n        }\n      }\n      if (touchEndX > touchStartX + 50) {\n        // Swipe right - prev\n        if (currentIndex > 0) {\n          currentIndex--;\n          updateSlide();\n        }\n      }\n    }\n    \n    // Recalculer lors du redimensionnement\n    let resizeTimer;\n    window.addEventListener('resize', () => {\n      clearTimeout(resizeTimer);\n      resizeTimer = setTimeout(() => {\n        // Ajuster l'index si on passe de mobile √† desktop\n        const cardsVisible = getCardsVisible();\n        if (currentIndex >= totalCards - cardsVisible) {\n          currentIndex = Math.max(0, totalCards - cardsVisible);\n        }\n        updateSlide();\n      }, 250);\n    });\n    \n    // Initialiser apr√®s le chargement\n    setTimeout(updateSlide, 100);\n  })();\n</script>",
